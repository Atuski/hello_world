#test6:
def fac(x):
    if x == 1 or x == 2:
        return 1
    else:
        return fac(x-1) + fac(x-2)
print(fac(10))
#test7:
a = [1,2,3]
b = a[:]
print('a列表是：%s'%a)
print('b列表是：%s'%b)
#test8:
for i in range(1,10):
    for j in range(1,i+1):
        print('%d*%d=%d'%(i,j,i*j),end = ' ')
    print('\n')
