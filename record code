#test22:
for i in range(ord('x'),ord('z')+1):
    for j in range(ord('x'),ord('z')+1):
         for k in range(ord('x'),ord('z')+1):
                if (i != j) and (i != k) and (j != k):#确定互不重复的x,y,z组合方式
                    if (i != ord('x')) and (k != ord('x')) and (k != ord('z')):#保证满足条件“a说他不和x比，c说他不和x,z比”
                        print('a-->%s,b-->%s,c-->%s'%(chr(i),chr(j),chr(k)))

#test23:
i = 3
j = 0
while i > 0:
    for _ in range(i):
        print(' ',end = '')
    for _ in range((j * 2)+1):
        print('*',end = '')
    
    i -= 1
    j += 1
    print()
while j > 0:
    i += 1
    j -= 1
    for _ in range(i):
        print(' ',end = '')
    for _ in range((j * 2)+1):
        print('*',end = '')
    print()
#test24:
a = 2.0
b = 1.0
sum = 0.0
for i in range(1,21):
    sum += a / b
    t = a
    a = a + b#分子等于前一项分子分母之和
    b = t#分母等于前一项分子
print(sum)
#test25:
from functools import reduce
sum = 0
for i in range(1,21):
    sum += reduce(lambda x, y: x * y,list(range(1,i+1)))
print(sum)
#test26:
def fact(n):
    if n == 0:
        return 1
    else :
        return fact(n-1)*n
print(fact(5))
#test27:
def output(s,l):
    if l == 0:
        return
    print(s[l-1])
    output(s,l-1)
s = input('please input a string:\n')
l = len(s)
output(s,l)
#test28:
def age(n):
    if n == 1:
        return 10
    else:
        return age(n-1) + 2
print(age(5))
#test29: 
n = int(input('please input a number：\n'))
l = []
i = 1
while n > 10:
    l.append(n % 10)
    n = n // 10
    i += 1
l.append(n)
print('%d位数：'%i,end = ' ')
for each in l :
    print(each,end = ' ')
#test30:
a = int(input("请输入一个数字:\n"))
x = str(a)
flag = True 
for i in range(len(x)//2):
    if x[i] != x[-i - 1]:
        flag = False
        break
if flag:
    print ("%d 是一个回文数!" % a)
else:
    print ("%d 不是一个回文数!" % a)
#test31:
letter = raw_input("please input:")
#while letter  != 'Y':
if letter == 'S':
    print ('please input second letter:')
    letter = raw_input("please input:")
    if letter == 'a':
        print ('Saturday')
    elif letter  == 'u':
        print ('Sunday')
    else:
        print ('data error')
    
elif letter == 'F':
    print ('Friday')
    
elif letter == 'M':
    print ('Monday')
    
elif letter == 'T':
    print ('please input second letter')
    letter = raw_input("please input:")
 
    if letter  == 'u':
        print ('Tuesday')
    elif letter  == 'h':
        print ('Thursday')
    else:
        print ('data error')
        
elif letter == 'W':
    print ('Wednesday')
else:
    print ('data error')
