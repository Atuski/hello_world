#test11:
def rab(n):
    if n == 1 or n ==2:
        return 1
    else:
        return rab(n - 1) + rab(n - 2)
for i in range(1,42):
    print('%10d'%rab(i),end = ' ')
    if i % 5 == 0:
        print()
#test12:
import math
def isprime(n):
    for i in range(2,int(math.sqrt(n))+1):
        if n % i == 0:
            return 1
j = 0
for i in range(101,201):
    if isprime(i) != 1:
        print(i , end = ' ')
        j += 1
        if j % 5 == 0:
            print('\n')
#test13:
def  daffodils(n):
    l = []
    m = n
    h = 0
    while n > 10:
        l.append(n % 10)
        n = n // 10
    l.append(n)
    for i in l:
        h += i**3
    if h == m:
        return 1
    else:
        return 0
    
for i in range(100,1000):
    if daffodils(i) == 1:
        print(i)  
#test14:
from math import sqrt
def prime(n):
    temp = 1
    if n == 0:
        return 0
    for i in range(2,int(sqrt(n)) + 1):
        if n % i == 0:
            return 0
    return 1
m = 2
s = [] #包含所有因数的列表
n = int(input('please input a number:'))
p = n
if prime(n) == 1:
        s.append(str(m))#为素数直接储存结束
else:
    while n > m - 1:
        if n % m == 0:
            s.append(str(m))#因数储存进list中
            n = n / m
        else:
            m += 1
print('%d='% p,end = '' )
print('*'.join(s))
  
#test15:
score = int(input('please input your score:'))
